{
  "compilerOptions": {
    "listEmittedFiles": true,
    "target": "ES2022",                          
    "module": "CommonJS",
    "moduleResolution": "node", 
    "lib": ["ESNext"],                   
    "sourceMap": true,
    "esModuleInterop": true,                                             
    "forceConsistentCasingInFileNames": true,          
    "strict": false, 
    "skipLibCheck": true , 
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "outDir": "./src"
  },

}

