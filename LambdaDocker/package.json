{
  "name": "Lambda-docker",
  "version": "1.0.0",
  "main": "server.js",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=local ts-node-dev server.ts",
    "migration": "ts-node-dev node_modules/.bin/typeorm migration:create ./migrations/${FILE:-migration}",
    "migrate": "node_modules/.bin/typeorm migration:run -d data-source.js",
    "migrateLocal": "ts-node-dev node_modules/.bin/typeorm migration:run -d data-source.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "reflect-metadata": "^0.1.13",
    "serverless-http": "^3.1.0",
    "sqlite3": "^5.1.2",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.6.0",
    "typeorm": "^0.3.10",
    "typescript": "^4.9.3",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.10",
    "@types/sequelize": "^4.28.14",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  }
}
